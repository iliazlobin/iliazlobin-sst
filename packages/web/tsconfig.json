{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [{ "name": "next" }],
    "paths": {
      "@iliazlobin/web/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "sst-env.d.ts",
    "my-env.d.ts",
    "**/*.tss",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "test/example.mjs"
  ],
  "exclude": ["node_modules"]
}
