{
  "name": "@iliazlobin/standalone",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "sst build",
    "console": "sst console",
    "deploy": "sst deploy",
    "eslint": "eslint \"**/*.+(ts|tsx)\"",
    "eslint:fix": "eslint --fix \"**/*.+(ts|tsx)\"",
    "prettier": "prettier --check \"**/*.+(ts|tsx|json|yml|yaml|md|css)\"",
    "prettier:fix": "prettier --write \"**/*.+(ts|tsx|json|yml|yaml|md|css)\"",
    "remove": "sst remove",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "eslint-plugin-prettier": "^5.1.3"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.1.4",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/node18": "^18.2.3",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "aws-cdk-lib": "2.132.1",
    "constructs": "10.3.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.4.12",
    "sst": "^2.41.4",
    "typescript": "^5.4.3"
  },
  "pnpm": {
    "patchedDependencies": {
      "sst@2.41.4": "patches/sst@2.41.4.patch"
    }
  }
}
