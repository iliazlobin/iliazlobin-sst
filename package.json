{
  "name": "@iliazlobin/sst",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "sst build",
    "deploy": "sst deploy --stage prod",
    "dev": "sst dev",
    "eslint": "eslint \"**/*.+(ts|tsx)\"",
    "eslint:fix": "eslint --fix \"**/*.+(ts|tsx)\"",
    "prettier": "prettier --check \"**/*.+(ts|tsx|json|yml|yaml|md|css)\"",
    "prettier:fix": "prettier --write \"**/*.+(ts|tsx|json|yml|yaml|md|css)\"",
    "remove": "sst remove",
    "typecheck": "tsc --noEmit",
    "types": "sst types"
  },
  "dependencies": {
    "moment": "^2.30.1",
    "zod-to-json-schema": "^3.22.5"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^20.12.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "aws-cdk-lib": "2.132.1",
    "colors": "^1.4.0",
    "constructs": "10.3.0",
    "dotenv": "^16.4.5",
    "sst": "^2.41.4",
    "typescript": "^5.4.3"
  },
  "pnpm": {
    "patchedDependencies": {
      "sst@2.41.4": "patches/sst@2.41.4.patch"
    }
  }
}
